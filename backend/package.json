{
  "name": "medusa-2.0-boilerplate-backend",
  "version": "0.0.2",
  "description": "A Prebaked Medusa 2.0 backend",
  "author": "Rasmus Puls",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "ib": "init-backend",
    "start": "init-backend && cd .medusa/server && medusa start --verbose",
    "dev": "medusa develop",
    "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.12.1",
    "@medusajs/cli": "2.12.1",
    "@medusajs/dashboard": "2.12.1",
    "@medusajs/draft-order": "2.12.1",
    "@medusajs/framework": "2.12.1",
    "@medusajs/medusa": "2.12.1",
    "@medusajs/notification-sendgrid": "2.12.1",
    "@medusajs/payment-stripe": "2.12.1",
    "@medusajs/workflow-engine-redis": "2.12.1",
    "@mikro-orm/core": "~6.4.3",
    "@mikro-orm/knex": "~6.4.3",
    "@mikro-orm/migrations": "~6.4.3",
    "@mikro-orm/postgresql": "~6.4.3",
    "@react-email/components": "^1.0.1",
    "@rokmohar/medusa-plugin-meilisearch": "^1.3.5",
    "@tiptap/extension-color": "^3.15.3",
    "@tiptap/extension-image": "^3.15.3",
    "@tiptap/extension-table": "^3.15.3",
    "@tiptap/extension-text-style": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "awilix": "^12.0.5",
    "medusajs-launch-utils": "^0.0.18",
    "minio": "^8.0.6",
    "pg": "^8.16.3",
    "resend": "^6.5.2",
    "ulid": "^3.0.1"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.1",
    "@mikro-orm/cli": "~6.4.3",
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "jest": "^30.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-email": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "engines": {
    "node": "24.x"
  },
  "trustedDependencies": [
    "@medusajs/telemetry",
    "@swc/core",
    "protobufjs",
    "unrs-resolver"
  ]
